{% extends "layout.nunjucks" %}
{% set page = "Work" %}

{% block content %}

  {% import 'macros/nav-macro.nunjucks' as nav %}
  {{nav.active('dev')}}

  <div class="dev-gallery">
    {% for dev_item in dev_items %}
    <div class="gallery-item">
      <div class="gallery-img-wrapper">
        <img src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-src="{{dev_item.src}}" alt="{{dev_item.title}}" class="lazyload" />
      </div>
      <div class="proj-title desc-item">
        <a class="proj-link" href="{{dev_item.link}}" target="_blank" rel="noopener">{{dev_item.title}}</a>
      </div>
      <div class="proj-tech desc-item">Tech: {{dev_item.technologies}}</div>
      {% if dev_item.award1 %}
        <div class="proj-awards desc-item">Awards:
          <a href="{{dev_item.award1}}" target="_blank" rel="noopener">GEN</a> &
          <a href="{{dev_item.award2}}" target="_blank" rel="noopener">IIB</a>
        </div>
      {% endif %}
    </div>
    {% endfor %}
  </div>

{% endblock %}
